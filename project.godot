; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Zero Sievert GNR v2"
config/description="Zero Sievert GNR (Gun Name Replacer)

A program that makes replacing the weapon names in the game Zero Sievert less tedious and risky."
config/version="1.0.0"
run/main_scene="res://main.tscn"
config/use_custom_user_dir=true
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0.270588, 0.156863, 0.235294, 1)
boot_splash/image="res://Icons/icon_256.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Icons/icon_256.png"

[autoload]

Globals="*res://globals.gd"

[display]

window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Zero-Sievert-GNRv2"

[editor_plugins]

enabled=PackedStringArray("res://addons/toggle_external_editor/plugin.cfg")

[gui]

theme/custom="res://default_theme.tres"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
